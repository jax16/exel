{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
"semi":"off",
"arrow-parens":"off",
"linebreak-style":"off",
"require-jsdoc":"off",
"indent" :"off",
"eol-last":"off",
"comma-dangle":"off",
"key-spacing":"off",
"object-curly-spacing":"off",
"no-trailing-spaces":"off"
  },
 
  "extends": ["eslint:recommended", "google"]
  
}